package com.lsm.web.board;

public class ReviewBoardFileVO {

	private int rbfNo;
	private int rNo;
	private String rbfPath;
	private String rbfName;
	private String rbfSize;
	private String rbfFormat;
	
	public int getRbfNo() {
		return rbfNo;
	}
	public void setRbfNo(int rbfNo) {
		this.rbfNo = rbfNo;
	}
	public int getrNo() {
		return rNo;
	}
	public void setrNo(int rNo) {
		this.rNo = rNo;
	}
	public String getRbfPath() {
		return rbfPath;
	}
	public void setRbfPath(String rbfPath) {
		this.rbfPath = rbfPath;
	}
	public String getRbfName() {
		return rbfName;
	}
	public void setRbfName(String rbfName) {
		this.rbfName = rbfName;
	}
	public String getRbfSize() {
		return rbfSize;
	}
	public void setRbfSize(String rbfSize) {
		this.rbfSize = rbfSize;
	}
	public String getRbfFormat() {
		return rbfFormat;
	}
	public void setRbfFormat(String rbfFormat) {
		this.rbfFormat = rbfFormat;
	}
	
}
