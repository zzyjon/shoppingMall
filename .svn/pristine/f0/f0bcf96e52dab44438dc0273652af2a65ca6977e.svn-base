	<!-- content page -->
	<section class="bgwhite p-t-60">
		<div class="container">
			<div class="row">
				<div class="col-md-8 col-lg-10 p-b-75">
					<div class="p-r-50 p-r-0-lg">
						<!-- item blog -->
						<table class="table-shopping-cart">
							<tr class="table-head">
								<th class="column-1"></th>
								<th class="column-1">상품</th>
								<th class="column-4">제목</th>
								<th class="column-1">작성자</th>
								<th class="column-1">조회수</th>
								<th class="column-4">작성일</th>
							</tr>
							
							<c:forEach var="rows" items="${reviewList }" varStatus="cntno">
							<tr class="table-row">
								<td class="column-1">${cntno.count }</td>
								<td class="column-1"><a href="/user/page/product/detail?pNo=${rows.pNo }"><img src="/product/${rows.pImg1}" style="width: 43%;height: 5%;"></a></td>
								<td class="column-4">
									<h6 class="p-b-11">
										<a href="/user/page/board/detail?rNo=${rows.rNo}" class="m-text10">
											${rows.rTitle }<span style="color:red;"><b>[${rows.replecount }]</b></span>
										</a>
										<c:if test="${rows.rImg1 != null}" ><i class="fa fa-fw fa-image"></i></c:if>
									</h6>
								</td>
								<td class="column-1">${rows.rWriter}(${rows.mId })</td>
								<td class="column-1">${rows.rViewcount }</td>
								<td class="column-4">${rows.rWriteAgo }</td>
							</tr>
							</c:forEach>
						</table>
					</div>


					<div class="form-inline">
						<!-- Pagination -->
						<div class="pagination flex-m flex-w p-t-26">
							<c:if test="${paging.currPage > 1 }">
								<a href="/user/page/review/search?keyword=${map.keyword }&searchOption=${map.searchOption }&currPage=${paging.currPage -1}" class="item-pagination flex-c-m trans-0-4"> Prev </a>
							</c:if>
								<c:forEach var="i" begin="${paging.startPageNum}" end="${paging.lastPageNum}">
										<c:choose>
											<c:when test="${i eq paging.currPage}">
													<a href="/user/page/review/search?keyword=${map.keyword }&searchOption=${map.searchOption }&currPage=${i}" class="item-pagination flex-c-m trans-0-4 active-pagination"> ${i}</a>
											</c:when>
											<c:otherwise>
													<a href="/user/page/review/search?keyword=${map.keyword }&searchOption=${map.searchOption }&currPage=${i}" class="item-pagination flex-c-m trans-0-4"> ${i}</a>
											</c:otherwise>
										</c:choose>
								</c:forEach>
							<c:if test="${paging.currPage < paging.totalPage }">
								<a href="/user/page/review/search?keyword=${map.keyword }&searchOption=${map.searchOption }&currPage=${paging.currPage +1}" class="item-pagination flex-c-m trans-0-4" > Next </a>
							</c:if>
						</div>
						
						<c:choose>
							<c:when test="${paging.totalCount > 0 }">
								<!-- result -->
								<div>${map.keyword }(으)로 검색 된 결과 ${paging.totalCount } 건 입니다</div>
							</c:when>
							<c:otherwise>
								<div>${map.keyword }(으)로 검색 된 결과가 없습니다 다시 검색해주세요</div>
							</c:otherwise>
						</c:choose>
						<!-- Search -->
						<form name="search" action="/user/page/review/search" >
						<div class="pos-relative bo11 of-hidden pull-right">
							<input class="s-text7 size16 p-l-23 p-r-50" type="text" name="keyword" placeholder="Search">
				
							<button type="button" class="flex-c-m size5 ab-r-m color1 color0-hov trans-0-4" id="btnSearch">
								<i class="fs-13 fa fa-search" aria-hidden="true"></i>
							</button>
							<div class="topbar-language rs1-select2">
								<select class="selection-1" name="searchOption">
									<option value="writer">작성자or아이디</option>
									<option value="title">제목</option>
								</select>
							</div>
						</div>
						</form>
						
					</div>
				</div>
			</div>
		</div>
	</section>
	
	


	<!-- 푸터 시작 -->
	<%@ include file="/WEB-INF/views/user/include/footer.jsp" %>
